{
  "name": "es2coffee",
  "version": "0.0.0-development",
  "description": "Convert ES6+ code to Coffeescript",
  "bin": {
    "es2coffee": "./bin/es2coffee.js"
  },
  "main": "build",
  "repository": "https://github.com/helixbass/es2coffee",
  "author": "Julian Rosse <@helixbass>",
  "license": "MIT",
  "keywords": [
    "coffeescript"
  ],
  "dependencies": {
    "@babel/core": "^7.0.0-beta.46",
    "babylon": "^7.0.0-beta.46",
    "lodash": "^4.17.10",
    "prettier": "../prettier",
    "prettier-plugin-coffeescript": "../prettier-plugin-coffeescript/"
  },
  "scripts": {
    "build": "coffee -o build/ -c src",
    "watch": "coffee --watch -o build/ -c src",
    "lint": "eslint src/*.coffee src/*/*.coffee",
    "test": "jest"
  },
  "devDependencies": {
    "eslint": "^5.0.1",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-coffee": "file:../eslint-plugin-coffee",
    "eslint-plugin-prettier": "^2.6.2",
    "jest": "^23.3.0"
  }
}
